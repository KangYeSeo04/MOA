{
  "expo": {
    "name": "moa",
    "slug": "moa",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/moa_logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#f57c00"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/moa_logo.png",
        "backgroundColor": "#f57c00"
